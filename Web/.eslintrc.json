{
  "root": true,
  "extends": ["next/core-web-vitals"],
  "rules": {
    "@next/next/no-img-element": "off",
    "react/jsx-key": "error",
    "react/jsx-no-useless-fragment": "warn",
    "prefer-const": "error",
    "eqeqeq": ["error", "smart"],
    "no-console": ["warn", { "allow": ["warn", "error"] }]
  }
}
